{
  "name": "pattern-projector",
  "version": "0.1.0",
  "private": true,
  "main": "pp_electron.js",
  "repository": "git@github.com:courtneypattison/pattern-projector.git",
  "author": "courtneypattison <courtneyp@ttison.ca>",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "test:unit": "jest",
    "cypress:open": "cypress open",
    "postinstall": "patch-package",
    "electron:start": "npx electron pp_electron.js",
    "dev:electron": "cross-env ELECTRON_START_URL=http://localhost:3000 concurrently --kill-others \"npm run dev\" \"wait-on http://localhost:3000 && npx electron pp_electron.js\"",
    "build:electron": "npm run build && npm run export && electron-builder --config electron-builder.json build --publish never"
  },
  "dependencies": {
    "@types/node": "20.6.2",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "@vercel/analytics": "^1.1.3",
    "@vercel/speed-insights": "^1.0.9",
    "autoprefixer": "10.4.15",
    "canvas": "^2.11.2",
    "electron-serve": "^1.3.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "ml-matrix": "^6.10.5",
    "next": "13.4.19",
    "next-intl": "^3.9.0",
    "next-pwa": "^5.6.0",
    "postcss": "8.4.29",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-full-screen": "^1.1.1",
    "react-pdf": "^7.7.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.5",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.3",
    "electron": "^29.0.1",
    "electron-builder": "^24.12.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "3.2.5",
    "wait-on": "^7.2.0",
    "ts-node": "^10.9.2"
  }
}
